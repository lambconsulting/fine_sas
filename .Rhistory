acplot <- function(v,ds,dst) {
v.form <- as.formula(paste("Surv(t,e==1) ~ pspline(",v,",df=4)",sep=""))
ph.fit <- coxph(v.form,data=ds)
termplot(ph.fit,term=1,se=TRUE,ylab='Log Hazard',rug=TRUE,xlab=v,main=dst)
}
